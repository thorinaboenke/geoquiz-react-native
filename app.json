{
  "expo": {
    "name": "geoquiz",
    "slug": "geoquiz",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/planet-earth.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "androidStatusBar": {
      "backgroundColor": "#FFFFFF",
      "translucent": true,
      "barStyle": "dark-content"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/globe_1.png"
    }
  }
}
